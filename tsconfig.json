{
  "compilerOptions": {
    "module": "esnext",
    "lib": [
      "es2020",
      "es2019",
      "es2018",
      "es2017",
      "es2016",
      "es2015",
      "dom",
      "dom.iterable"
    ],
    "jsx": "preserve",
    "noImplicitAny": false,
    "allowJs": true,
    "removeComments": true,
    "target": "es6",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "isolatedModules": true
  },
  "rules": {
    "no-console": false,
    "interface-name": [
      true,
      "never"
    ]
  }
}